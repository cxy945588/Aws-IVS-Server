{
  "name": "ivs-media-server",
  "version": "1.0.0",
  "description": "Media Server for AWS IVS Real-time multi-stage streaming",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "aws",
    "ivs",
    "media-server",
    "webrtc",
    "whip",
    "streaming"
  ],
  "author": "AWS IVS Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "ws": "^8.18.0",
    "axios": "^1.7.0",
    "dotenv": "^16.4.5",
    "winston": "^3.17.0",
    "helmet": "^8.0.0",
    "cors": "^2.8.5",
    "@aws-sdk/client-ivs-realtime": "^3.500.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^22.0.0",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1"
  }
}
